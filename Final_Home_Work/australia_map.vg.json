{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Ladder score of Australia from 2010-2019",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
    "url": "australia.json",
    "format": {"type": "topojson", "feature": "ne_10m_admin_0_countries"}
    },
    "transform": [
    {
    "lookup": "properties.NAME",
    "from": {
    "data": {
    "url": "Austrlia_map_data.csv"
    },
    "key": "country name",
    "fields": ["year","life ladder"]
}
}

],
"mark": {"type": "geoshape"},
"encoding": {
"color": {
"field": "life ladder",
"type": "quantitative",
"scale": {"type": "log"}
},
"tooltip": [
{"field": "properties.NAME", "type": "nominal", "title":
"Country"},
{"field": "life ladder", "type": "quantitative"}
]
}
}

